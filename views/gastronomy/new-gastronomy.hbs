<h1>New Gastronomy</h1>
<div>
 
  <form action="/gastronomy/create" method="POST" enctype="multipart/form-data">
    <label>
      Name:
      <input type="text" name="name" />
    </label>
    <label>
      type:
      <select name="type">
        <option>Restaurant</option>
        <option>Bar</option>
      </select>
    </label>
    <label>
      Description:

      <input type="text" name="description" />
    </label>
    <label>
      price:
      <select name="price">
        <option>Economic</option>
        <option>Normal</option>
        <option>Expensive</option>
      </select>
    </label>
    <label>
      Food:
      <input type="text" name="food" />
    </label>
    <label>
      Image:
      <input type="file" name="image" class="form-control-file" />
    </label>

       <input type="text" id="address" class="address">
      <button class="submit">enviar</button>
        <br>


    <span class="latitudeCenter">40.4206213320784</span>
    <span class="longitudeCenter">-3.706324171610527</span>


    <div id="map"></div>

     <label class="labelLocation">
      Location:
      <input type="text" name="lat" class="latitudeText" />
      <input type="text" name="lng" class="longitudeText" />
    </label>
    <label>
      Review:
      <input type="text" name="" />
    </label>
    <label>
      Website:
      <input type="text" name="webSite" />
    </label>
    <label>
      Opening Time:
      <input type="text" name="openingTime" />
    </label>
    <label>
      ClosingTime:
      <input type="text" name="closingTime" />
    </label>
    <input type="submit" />
    {{#if errorMessage}}
      <p class="error">{{errorMessage}}</p>
    {{/if}}
  </form>
</div>


<script src="../../js/main.js"> </script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA2EY6nvc3be8-6agfTwW2PNHPH0GX3dg8&callback=initMapNew"></script>